{
  "version": 2,
  "builds": [
    {
      "src": "dist/my-ssr-app/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/my-ssr-app/browser/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "dist/my-ssr-app/server/server.mjs"
    }
  ]
}
